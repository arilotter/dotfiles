alignment top_right
minimum_size 1266 5
maximum_width 1240
background no
border_width 1
cpu_avg_samples 2
default_bar_size 50 3
default_color #9aacc3
default_outline_color #9aacc3
default_shade_color #9aacc3
draw_borders no
draw_graph_borders yes
draw_outline no
draw_shades no
use_xft yes
xftfont	Terminus:size=8
override_utf8_locale yes
gap_x 0
gap_y 2
net_avg_samples 2
no_buffers yes
out_to_console no
out_to_stderr no
extra_newline no
own_window yes
own_window_class Conky
own_window_type panel
stippled_borders 0
update_interval 1.0
uppercase no
use_spacer no
show_graph_scale no
show_graph_range no
own_window_colour 0b0d22
own_window_argb_visual yes
own_window_argb_value 255
double_buffer yes

color0 1F1F1F
color1 f5f2df

TEXT
$alignr${offset -5}${voffset 2}\
${color1}music * ${execi 10 lastfm}\
${color0} // \
${color1}vol * ${execi 2 getvolume}\
${color0} // \
${color1}batt * ${execi 10 getbattery}\
${color0} // \
${color1}wlan * ${wireless_essid wlan0}\
${color0} // \
${color1}${time %I:%M%P}\
${voffset -2}
